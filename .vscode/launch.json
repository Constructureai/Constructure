{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Backend",
        "program": "${workspaceFolder}/backend/server.js",
        "envFile": "${workspaceFolder}/backend/.env.development"
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Frontend",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/frontend"
      }
    ]
  }